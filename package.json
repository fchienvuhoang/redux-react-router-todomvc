{
  "name": "redux-react-router-todomvc",
  "version": "1.0.0",
  "description": "An implementation of TodoMVC using React, Redux, React-Router, and more!",
  "repository": "tough-griff/redux-react-router-todomvc",
  "author": "Griffin Yourick",
  "license": "MIT",
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "check": "npm run lint && npm run test",
    "clean": "rimraf assets coverage",
    "lint": "eslint --ext=.js,.jsx js test",
    "start": "babel-node server.js",
    "test:cov": "NODE_ENV=test babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --recursive",
    "test:watch": "NODE_ENV=test npm run test -- --watch",
    "test": "NODE_ENV=test mocha --compilers=js:babel-core/register --recursive"
  },
  "dependencies": {
    "babel-core": "^5.8.25",
    "camel-case": "^1.1.2",
    "capitalize": "^1.0.0",
    "classnames": "^2.1.5",
    "constant-mirror": "^1.0.5",
    "history": "^1.12.5",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.1.1",
    "jsdom": "^6.5.1",
    "react": "^0.14.0",
    "react-dnd": "^2.0.0",
    "react-dnd-html5-backend": "^2.0.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.2",
    "redux-thunk": "^1.0.0",
    "todomvc-app-css": "^2.0.1",
    "todomvc-common": "^1.0.2"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "eslint": "^1.7.1",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-react": "^3.5.1",
    "expect.js": "^0.3.1",
    "express": "^4.13.3",
    "fetch-mock": "^2.1.0",
    "isparta": "^3.1.0",
    "json-server": "^0.8.2",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "morgan": "^1.6.1",
    "node-libs-browser": "^0.5.3",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.1",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  }
}
